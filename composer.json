{
    "name": "jorisvanw/laravel-recurly",
    "homepage": "https://github.com/JorisvanW/laravel-recurly",
    "description": "A simple Laravel 5 service provider for including the Recurly PHP client.",
    "keywords": ["laravel","laravel 5","recurly","recurring","billing","api"],
    "license":"MIT",
    "authors":[
        {
            "name":"Joris van Willigen",
            "email":"me@jorisvanw.me",
            "homepage":"https://jorisvanw.me"
        },
        {
            "name":"Chris Thompson",
            "email":"christhompsontldr@gmail.com",
            "homepage":"https://christhompsontldr.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "recurly/recurly-client": "2.13.*"
    },
    "autoload": {
        "psr-4": {
            "JorisvanW\\LaravelRecurly\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JorisvanW\\LaravelRecurly\\ServiceProvider"
            ]
        }
    },
}
